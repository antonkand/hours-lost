<script src="/socket.io/socket.io.js"></script>
<script>
  var socket = io.connect('/hours-lost');
  socket.on('socket:connection', function (data) {
      console.log(data);
      var log = ('hours-lost:\n   client socket ' + socket.ids + '\n   in namespace ' + socket.nsp + ' connected.');
      socket.emit('socket:connection', log);
  });
  socket.on('twitter:username', function (data) {
      console.log('twitter username received: ' + data);
  });
</script>
