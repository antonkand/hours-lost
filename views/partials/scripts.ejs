<script src="/socket.io/socket.io.js"></script>
<script>
  var socket = io.connect('/hours-lost');
  socket.on('socket:connection', function (data) {
      console.log(data);
      var msg = ('hours-lost: client socket ' + socket.ids + ' successfully connected to server.');
      socket.emit('socket:connection', msg);
      console.log(socket);
  });
</script>
