<script src="/socket.io/socket.io.js"></script>
<script>
  var main = io.connect('/hours-lost');
  var twitter = io.connect('/hours-lost/twitter');
  main.on('socket:connection', function (data) {
      console.log(data);
      var log = ('hours-lost:\n   client socket ' + main.ids + '\n   in namespace ' + main.nsp + ' connected.');
      main.emit('socket:connection', log);
  });
  twitter.on('twitter:username', function (data) {
      console.log('twitter username received: ' + data);
  });
</script>
